{
  "name": "proyecto-odontologia",
  "version": "1.0.0",
  "description": "Sistema completo de gestión de citas odontológicas",
  "scripts": {
    "install-backend": "cd odontologia-backend && npm install",
    "install-frontend": "cd odontologia-app && npm install",
    "install-all": "npm run install-backend && npm run install-frontend",
    "start-backend": "cd odontologia-backend && npm start",
    "start-frontend": "cd odontologia-app && ng serve",
    "dev": "concurrently \"npm run start-backend\" \"npm run start-frontend\""
  },
  "keywords": ["odontologia", "citas", "angular", "nodejs", "mongodb"],
  "author": "Stefanny26",
  "license": "MIT"
}